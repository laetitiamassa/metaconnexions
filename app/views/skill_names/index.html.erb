<p id="notice"><%= notice %></p>

<h1>Listing Skill Names</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skill_names.each do |skill_name| %>
      <tr>
        <td><%= skill_name.name %></td>
        <td><%= link_to 'Show', skill_name %></td>
        <td><%= link_to 'Edit', edit_skill_name_path(skill_name) %></td>
        <td><%= link_to 'Destroy', skill_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill name', new_skill_name_path %>
